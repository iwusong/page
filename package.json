    "@vue/eslint-config-prettier": "^9.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.47",
    "prettier": "^3.2.5",
    "vite": "^5.3.1",
    "wrangler": "^3.80.4"
